{
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "jest": "^29.7.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "**/tests/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
